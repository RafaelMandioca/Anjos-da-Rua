{
  "name": "anjosdaruaproject",
  "version": "1.0.0",
  "scripts": {
    "scss": "sass static/scss/main.scss:static/css/app.css --watch",
    "dev": "browser-sync start --config bs-config.js",
    "django-dev": "python manage.py runserver --settings=anjosdaruaproject.settings.development",
    "start": "npm-run-all --parallel scss dev django-dev"
  },
  "dependencies": {
    "browser-sync": "^3.0.4",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
